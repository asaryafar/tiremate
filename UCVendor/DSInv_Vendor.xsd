<?xml version="1.0" standalone="yes" ?>
<xs:schema id="DSInv_Vendor" targetNamespace="http://www.tempuri.org/DSInv_Vendor.xsd" xmlns:mstns="http://www.tempuri.org/DSInv_Vendor.xsd"
	xmlns="http://www.tempuri.org/DSInv_Vendor.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="DSInv_Vendor" msdata:IsDataSet="true" msdata:Locale="fa-IR">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="inv_vendor">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="cod_vendor" type="xs:string" />
							<xs:element name="desc_vendor" type="xs:string" minOccurs="0" />
							<xs:element name="address" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:inv_vendor" />
			<xs:field xpath="mstns:cod_vendor" />
		</xs:unique>
	</xs:element>
</xs:schema>
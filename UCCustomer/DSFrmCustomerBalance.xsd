<?xml version="1.0" standalone="yes" ?>
<xs:schema id="DSFrmCustomerBalance" targetNamespace="http://www.tempuri.org/DSFrmCustomerBalance.xsd"
	xmlns:mstns="http://www.tempuri.org/DSFrmCustomerBalance.xsd" xmlns="http://www.tempuri.org/DSFrmCustomerBalance.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
	attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="DSFrmCustomerBalance" msdata:IsDataSet="true" msdata:Locale="fa-IR">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="service_center_dtl">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="InvoiceNo" type="xs:string" />
							<xs:element name="InvoiceDate" type="xs:string" minOccurs="0" />
							<xs:element name="OriginalAmount" type="xs:double" minOccurs="0" />
							<xs:element name="Payment" type="xs:double" minOccurs="0" />
							<xs:element name="AmountDue" type="xs:double" minOccurs="0" />
							<xs:element name="OldPayment" type="xs:double" minOccurs="0" />
							<xs:element name="DueDate" type="xs:string" minOccurs="0" />
							<xs:element name="PastDue" type="xs:int" minOccurs="0" />
							<xs:element name="DueDateYYYYMMDD" type="xs:string" minOccurs="0" />
							<xs:element name="FlagForBalance" type="xs:boolean" minOccurs="0" />
							<xs:element name="InvoiceDateYYYYMMDD" type="xs:string" minOccurs="0" />
							<xs:element name="Type" type="xs:int" minOccurs="0" />
							<xs:element name="InvoiceNoForOrder" type="xs:string" minOccurs="0" />
							<xs:element name="ID_payment" type="xs:int" minOccurs="0" />
							<xs:element name="Cancel_flag" type="xs:boolean" minOccurs="0" />
							<xs:element name="TypeOfRecord" type="xs:int" minOccurs="0" />
							<xs:element name="Paid_amount" type="xs:double" minOccurs="0" />
							<xs:element name="STORENO" type="xs:string" minOccurs="0" />
							<xs:element name="SHORTNAME" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Store_Setup">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="STORENO" type="xs:string" />
							<xs:element name="STORENAME" type="xs:string" minOccurs="0" />
							<xs:element name="SHORTNAME" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
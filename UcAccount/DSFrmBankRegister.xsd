<?xml version="1.0" standalone="yes" ?>
<xs:schema id="DSFrmBankRegister" targetNamespace="http://www.tempuri.org/DSFrmBankRegister.xsd"
	xmlns:mstns="http://www.tempuri.org/DSFrmBankRegister.xsd" xmlns="http://www.tempuri.org/DSFrmBankRegister.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
	attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="DSFrmBankRegister" msdata:IsDataSet="true" msdata:Locale="fa-IR">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="bas_banks">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_bank" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:short" />
							<xs:element name="name_bank" type="xs:string" minOccurs="0" />
							<xs:element name="share_all_store" type="xs:boolean" minOccurs="0" />
							<xs:element name="store_no" type="xs:string" minOccurs="0" />
							<xs:element name="account_no" type="xs:string" minOccurs="0" />
							<xs:element name="address" type="xs:string" minOccurs="0" />
							<xs:element name="zip" type="xs:string" minOccurs="0" />
							<xs:element name="phone_1" type="xs:string" minOccurs="0" nillable="true" />
							<xs:element name="extension_1" type="xs:string" minOccurs="0" />
							<xs:element name="phone_type_1" type="xs:string" minOccurs="0" />
							<xs:element name="phone_main_1" type="xs:boolean" minOccurs="0" />
							<xs:element name="phone_2" type="xs:string" minOccurs="0" nillable="true" />
							<xs:element name="extension_2" type="xs:string" minOccurs="0" />
							<xs:element name="phone_type_2" type="xs:string" minOccurs="0" />
							<xs:element name="phone_main_2" type="xs:boolean" minOccurs="0" />
							<xs:element name="phone_3" type="xs:string" minOccurs="0" />
							<xs:element name="extension_3" type="xs:string" minOccurs="0" />
							<xs:element name="phone_type_3" type="xs:string" minOccurs="0" />
							<xs:element name="phone_main_3" type="xs:boolean" minOccurs="0" />
							<xs:element name="phone_4" type="xs:string" minOccurs="0" />
							<xs:element name="extension_4" type="xs:string" minOccurs="0" />
							<xs:element name="phone_type_4" type="xs:string" minOccurs="0" />
							<xs:element name="phone_main_4" type="xs:boolean" minOccurs="0" />
							<xs:element name="phone_5" type="xs:string" minOccurs="0" />
							<xs:element name="extension_5" type="xs:string" minOccurs="0" />
							<xs:element name="phone_type_5" type="xs:string" minOccurs="0" />
							<xs:element name="phone_main_5" type="xs:boolean" minOccurs="0" />
							<xs:element name="email" type="xs:string" minOccurs="0" />
							<xs:element name="contact_name" type="xs:string" minOccurs="0" />
							<xs:element name="GL_Account" type="xs:string" minOccurs="0" />
							<xs:element name="start_check_no" type="xs:string" minOccurs="0" />
							<xs:element name="next_check_no" type="xs:string" minOccurs="0" />
							<xs:element name="default_bank" type="xs:boolean" minOccurs="0" />
							<xs:element name="inactive" type="xs:boolean" minOccurs="0" />
							<xs:element name="cod_account_type" type="xs:string" minOccurs="0" />
							<xs:element name="routing_numbers" type="xs:string" minOccurs="0" />
							<xs:element name="Bank_ID" type="xs:string" minOccurs="0" />
							<xs:element name="Web_Page" type="xs:string" minOccurs="0" />
							<xs:element name="User_ID" type="xs:string" minOccurs="0" />
							<xs:element name="Password" type="xs:string" minOccurs="0" />
							<xs:element name="Security_code" type="xs:string" minOccurs="0" />
							<xs:element name="online_banking" type="xs:boolean" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="BankRegister">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Date1" type="xs:date" minOccurs="0" />
							<xs:element name="Type" type="xs:string" minOccurs="0" />
							<xs:element name="Number" type="xs:string" minOccurs="0" />
							<xs:element name="Payee" type="xs:string" minOccurs="0" />
							<xs:element name="Gl_Account" type="xs:string" minOccurs="0" />
							<xs:element name="desc_GL_account" type="xs:string" minOccurs="0" />
							<xs:element name="debit_amount" type="xs:decimal" minOccurs="0" />
							<xs:element name="Cleared_bank" type="xs:boolean" minOccurs="0" />
							<xs:element name="deposit_amount" type="xs:decimal" minOccurs="0" />
							<xs:element name="Balance" type="xs:decimal" minOccurs="0" />
							<xs:element name="Memo" type="xs:string" minOccurs="0" />
							<xs:element name="CustomerVendorEmployeeOther" type="xs:int" minOccurs="0" />
							<xs:element name="CheckBankDepositBankTransaction" type="xs:int" minOccurs="0" />
							<xs:element name="Id_Check" type="xs:int" minOccurs="0" />
							<xs:element name="Id_Atm" type="xs:int" minOccurs="0" />
							<xs:element name="id_adjustment_no" type="xs:int" minOccurs="0" />
							<xs:element name="id_transfer" type="xs:int" minOccurs="0" />
							<xs:element name="id_debit_deposit_head" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="bank_check">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_check" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="id_bank" type="xs:short" minOccurs="0" />
							<xs:element name="no_of_check" type="xs:string" minOccurs="0" />
							<xs:element name="amount_check" type="xs:decimal" minOccurs="0" />
							<xs:element name="cod_vendor" type="xs:string" minOccurs="0" />
							<xs:element name="employee_code" type="xs:string" minOccurs="0" />
							<xs:element name="memo_check" type="xs:string" minOccurs="0" />
							<xs:element name="invoice_no" type="xs:string" minOccurs="0" />
							<xs:element name="check_status" type="xs:string" minOccurs="0" />
							<xs:element name="desc_vendor" type="xs:string" minOccurs="0" />
							<xs:element name="l_name" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="GL_payment_deposit">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID_payment" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="id_service_center" type="xs:string" minOccurs="0" />
							<xs:element name="refrence_no" type="xs:string" minOccurs="0" />
							<xs:element name="refer_type" type="xs:string" minOccurs="0" />
							<xs:element name="payment_type" type="xs:string" minOccurs="0" />
							<xs:element name="payment_amount" type="xs:decimal" minOccurs="0" />
							<xs:element name="flag_deposit_bank" type="xs:boolean" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="bank_check_dtl">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_check" type="xs:int" />
							<xs:element name="radif" type="xs:short" />
							<xs:element name="GL_Account" type="xs:string" minOccurs="0" />
							<xs:element name="amount" type="xs:decimal" minOccurs="0" />
							<xs:element name="memo_dtl" type="xs:string" minOccurs="0" />
							<xs:element name="desc_GL_account" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="bank_ATM">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_ATM" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="id_bank" type="xs:short" minOccurs="0" />
							<xs:element name="date_transaction" type="xs:string" minOccurs="0" />
							<xs:element name="amount_ATM" type="xs:decimal" minOccurs="0" />
							<xs:element name="cod_vendor" type="xs:string" minOccurs="0" />
							<xs:element name="memo_address" type="xs:string" minOccurs="0" />
							<xs:element name="id_debit_deposit_head" type="xs:int" minOccurs="0" />
							<xs:element name="ChargeCredit" type="xs:string" minOccurs="0" />
							<xs:element name="desc_vendor" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="bank_ATM_dtl">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_ATM" type="xs:int" />
							<xs:element name="radif" type="xs:short" />
							<xs:element name="GL_account" type="xs:string" minOccurs="0" />
							<xs:element name="amount" type="xs:decimal" minOccurs="0" />
							<xs:element name="memo_dtl" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="GL_debit_deposit">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_bank" type="xs:short" minOccurs="0" />
							<xs:element name="date_doc" type="xs:string" minOccurs="0" />
							<xs:element name="debit_amount" msdata:ReadOnly="true" type="xs:decimal" minOccurs="0" />
							<xs:element name="deposit_amount" msdata:ReadOnly="true" type="xs:decimal" minOccurs="0" />
							<xs:element name="GL_account" type="xs:string" minOccurs="0" />
							<xs:element name="desc_GL_account" type="xs:string" minOccurs="0" />
							<xs:element name="id_debit_deposit_head" msdata:ReadOnly="true" msdata:AutoIncrement="true"
								type="xs:int" />
							<xs:element name="memo" type="xs:string" minOccurs="0" />
							<xs:element name="memo2" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:bas_banks" />
			<xs:field xpath="mstns:id_bank" />
		</xs:unique>
		<xs:unique name="GL_payment_deposit_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:GL_payment_deposit" />
			<xs:field xpath="mstns:ID_payment" />
		</xs:unique>
		<xs:unique name="bank_ATM_dtl_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:bank_ATM_dtl" />
			<xs:field xpath="mstns:id_ATM" />
			<xs:field xpath="mstns:radif" />
		</xs:unique>
	</xs:element>
</xs:schema>